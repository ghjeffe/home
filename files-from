# Paths should be rooted from user's home directory
# Examples:
# .zshrc backs up ~/.zshrc
# .ssh/config backs up ~/.ssh/config

# Directories should have trailing /
# Examples:
# .config/autostart/ backups all files underneath


# Stuff to help backups
code/home/files-from

# KDE autostart stuff
.config/autostart/

# ZSH stuff
.zshrc
.zprofile
.oh-my-zsh/custom/
#.oh-my-zsh/.p10k.zsh

# Bash stuff
.bashrc
.bash_profile
.bash_logout

# VIM stuff
.vimrc

# SSH stuff
.ssh/config

# Git stuff
.gitconfig

# General configs
.config/htop
#.lesskey

# TMUX
.tmux.conf

# Systemd stuff
.config/systemd/user/ssh-agent.service
